@using System.Resources
@{
    Layout = null;
}
<!DOCTYPE html>
<!--
Template Name: Metronic - Responsive Admin Dashboard Template build with Twitter Bootstrap 3.0.3
Version: 1.5.5
Author: KeenThemes
Website: http://www.keenthemes.com/
Purchase: http://themeforest.net/item/metronic-responsive-admin-dashboard-template/4021469?ref=keenthemes
-->
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>Home</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta name="MobileOptimized" content="320">
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="@Url.Content("~/assets/plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/uniform/css/uniform.default.css")" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGIN STYLES -->
    <link href="@Url.Content("~/assets/plugins/gritter/css/jquery.gritter.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/fullcalendar/fullcalendar/fullcalendar.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/jqvmap/jqvmap/jqvmap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/jquery-easy-pie-chart/jquery.easy-pie-chart.css")" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL PLUGIN STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href="@Url.Content("~/assets/css/style-metronic.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/style-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/plugins.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/pages/tasks.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/themes/blue.css")" rel="stylesheet" type="text/css" id="style_color" />
    <link href="@Url.Content("~/content/validation.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/content/style.css")" rel="stylesheet" type="text/css" />

    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-header-fixed page-full-width" data-ng-app="app">
    <!-- BEGIN HEADER -->
    <div class="header navbar navbar-inverse navbar-fixed-top">
        <!-- BEGIN TOP NAVIGATION BAR -->
        <div class="header-inner">
            <!-- BEGIN LOGO -->
            <a class="navbar-brand" href="/">
                <img src="@Url.Content("~/assets/img/hippocratic-solutions-logo.png")" alt="Hippocratic Solutions Logo" class="img-responsive">
            </a>
            <!-- END LOGO -->
            <!-- BEGIN HORIZANTAL MENU -->
            <div class="hor-menu hidden-sm hidden-xs">
                @Html.Action("Menu", "Navigation")
            </div>
            <!-- END HORIZANTAL MENU -->
            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
            <a href="javascript:;" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <img src="@Url.Content("~/assets/img/menu-toggler.png")" alt="" />
            </a>
            <!-- END RESPONSIVE MENU TOGGLER -->
            <!-- BEGIN TOP NAVIGATION MENU -->
            <ul class="nav navbar-nav pull-right profile">
                <li><a href="/settings"> <i class="fa fa-cog settings"></i></a></li>
                <!-- BEGIN USER LOGIN DROPDOWN -->
                <li class="dropdown user">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                        <span class="username">
                            {{User.UserName}}
                        </span>
                        <i class="fa fa-angle-down"></i>
                        <div class="img-circular"></div>
                    </a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="javascript:;" id="trigger_fullscreen"><i class="fa fa-move"></i> Full Screen</a>
                        </li>
                        <li>
                            <a href="@Url.Action("Logout","Account")"><i class="fa fa-key"></i> Log Out</a>
                        </li>
                    </ul>
                </li>
                <!-- END USER LOGIN DROPDOWN -->
            </ul>
            <!-- END TOP NAVIGATION MENU -->
        </div>
        <!-- END TOP NAVIGATION BAR -->
    </div>
    <!-- END HEADER -->
    <div class="clearfix">
    </div>
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN EMPTY PAGE SIDEBAR -->
        <div class="page-sidebar navbar-collapse collapse">
            @Html.Action("CollapseMenu", "Navigation")
        </div>
        <!-- END EMPTY PAGE SIDEBAR -->
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
            <div class="page-content menu2">
                <!-- BEGIN ADD PATIENT BUTTON-->
                <div class="page-content home" data-ng-controller="homeController">
                    <h1>Which practice are you looking for?</h1>

                    <div class="row">
                        <div class="btn-group col-md-5" data-toggle="buttons">
                            <label class="btn btn-default col-md-4" data-ng-click="switch(allStatus.Active)">
                                <input type="radio" class="toggle"> Active
                            </label>
                            <label class="btn btn-default col-md-4" data-ng-click="switch(allStatus.Inactive)">
                                <input type="radio" class="toggle "> Inactive
                            </label>
                            <label class="btn btn-default col-md-4 active" data-ng-click="switch(allStatus.All)">
                                <input type="radio" class="toggle "> All Businesses
                            </label>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-3 search-box">
                            <input type="text" class="form-control" data-ng-model="keyword" placeholder="Search practice name or address by typing here...">
                            <i class="fa fa-search"></i>
                        </div>
                        <div class="col-md-6 pull-right text-right">
                            <button type="button" class="btn cgreen" data-ng-click="create()">Add New Practice<img src="@Url.Content("~/assets/img/add-icon.png")" style="margin-left: 10px;height:30px;" /></button>
                        </div>
                    </div>

                    <div class="row-fluid">
                        <table class="table table-hover" data-ng-show="!loading">
                            <tbody>
                                <tr data-ng-repeat="p in source" data-ng-click="select(p)" ng-class="{ td_practive_disabled : !p.ClickAble}">
                                    <td></td>
                                    <td>{{p.Name}}</td>
                                    <td>{{p.Address1}} {{p.Address2}} - {{p.City}}, {{p.State}} {{p.ZipCode}}</td>
                                    <td>|<i class="fa fa-angle-right pull-right"></i></td>
                                </tr>
                                <tr data-ng-show="source.length==0&&!loading">
                                    <td colspan="4">No records.</td>
                                </tr>
                            </tbody>
                        </table>
                        <div class="inbox-loading" data-ng-show="loading">
                            
                        </div>
                    </div>

                    <!-- END PAGE CONTENT -->
                </div>
                <!-- END PAGE CONTENT -->
            </div>
            <!-- END PAGE CONTENT WRAPPER-->
        </div>
        <!-- END PAGE CONTAINER-->
    </div>
    <!-- BEGIN FOOTER -->
    <div style="background-color: white;">
        <div class="footer">
            <div class="footer-inner">
                &copy;2014 Hippocrat Solutions LLC. All rights reserved.
            </div>
        </div>
    </div>
    <div class="modal fade" id="warning" tabindex="-1" role="dialog" aria-hidden="false" style="display:none;">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header bg-warning">
                    <i class="fa fa-times-circle"></i> Select Practice
                </div>
                <div class="modal-body">
                    <br />
                    <p>You can select up to 10 tabs, pelase remove one if you want to select this.</p>
                    <br />
                    <div class="button-group">
                        <a href="@Url.Action("index","patients")" class="btn btn-warning">Go to remove the practice</a>
                        or <span class="cancel btnCancel" data-dismiss="modal">Cancel</span>
                    </div>
                    <br><br>
                </div>
                <div class="modal-footer">

                </div>
            </div>
        </div>
    </div>

    <!-- END FOOTER -->
    <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
    <!-- BEGIN CORE PLUGINS -->
    <!--[if lt IE 9]>
    <script src="@Url.Content("~/assets/plugins/respond.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/excanvas.min.js")"></script>
    <![endif]-->
    <script src="@Url.Content("~/assets/plugins/jquery-1.10.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/plugins/jquery-migrate-1.2.1.min.js")" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui-1.10.3.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src="@Url.Content("~/assets/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/plugins/bootstrap-hover-dropdown/twitter-bootstrap-hover-dropdown.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/plugins/jquery-slimscroll/jquery.slimscroll.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/plugins/jquery.blockui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/plugins/jquery.cokie.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/assets/plugins/uniform/jquery.uniform.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/angular.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/angular-route.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/angular-cookies.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/app/app.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/app/services/userService.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/app/services/practiceService.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/app/controllers/appController.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/app/controllers/homeController.js")" type="text/javascript"></script>

    <!-- END CORE PLUGINS -->
    <!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->
</html>

